IBackend":{"kind":"article","abstract":[{"text":"SwiftCrossUI’s native Windows backend built on top of WinUI 3.","type":"text"}],"identifier":"doc:\/\/SwiftCrossUI\/documentation\/SwiftCrossUI\/WinUIBackend","title":"WinUIBackend","type":"topic","role":"article","url":"\/documentation\/swiftcrossui\/winuibackend"},"doc://SwiftCrossUI/documentation/SwiftCrossUI/UIKitBackend":{"identifier":"doc:\/\/SwiftCrossUI\/documentation\/SwiftCrossUI\/UIKitBackend","url":"\/documentation\/swiftcrossui\/uikitbackend","abstract":[{"text":"SwiftCrossUI’s native iOS and tvOS backend built on top of UIKit.","type":"text"}],"title":"UIKitBackend","type":"topic","kind":"article","role":"article"},"doc://SwiftCrossUI/documentation/SwiftCrossUI":{"identifier":"doc:\/\/SwiftCrossUI\/documentation\/SwiftCrossUI","role":"collection","kind":"symbol","title":"SwiftCrossUI","url":"\/documentation\/swiftcrossui","type":"topic","abstract":[{"type":"text","text":"Create cross-platform desktop apps for macOS, Linux, Windows, iOS and tvOS."}]}}}